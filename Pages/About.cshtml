@page
@model AboutModel
@{
    ViewData["Title"] = "About";
    Layout = "_Layout";
}
<div class="gb-breadcrumb gb-bg white-color about-bg">
    <div class="container">
        <div class="breadcrumb-info text-center">
            <div class="page-title">
                <h1>
                    <span class="before-top"></span>
